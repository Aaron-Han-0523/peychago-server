<!DOCTYPE html>
<html lang="ko">

<head>
    <%- include ("../partials/header.ejs") %>
</head>

<body class="layout-boxed remove-alt-menu">

    <%- include ("../partials/loader.ejs") %>

        <%- include ("../partials/nav.ejs") %>

            <!--  BEGIN MAIN CONTAINER  -->
            <div class="main-container sidebar-closed " id="container">

                <div class="overlay"></div>
                <div class="search-overlay"></div>

                <%- include ("../partials/sidebar.ejs") %>

                    <!--  BEGIN CONTENT AREA  -->
                    <div id="content" class="main-content">
                        <div class="layout-px-spacing">

                            <div class="middle-content container-xxl p-0">
                                <!-- BREADCRUMB -->
                                <div class="page-meta">
                                    <nav class="breadcrumb-style-one" aria-label="breadcrumb">
                                        <ol class="breadcrumb">
                                            <!--                                <li class="breadcrumb-item"><a href="#">Table</a></li>-->
                                            <li class="breadcrumb-item active" aria-current="page">Reception History
                                                Management</li>
                                            <li class="breadcrumb-item active" aria-current="page">Supplier Request</li>
                                        </ol>
                                    </nav>
                                </div>
                                <!-- /BREADCRUMB -->
                                <div class="col-lg-12 layout-top-spacing">
                                    <div class="input-group">
                                        <input type="text" class="form-control" placeholder="Title and content"
                                            aria-label="Recipient's username with two button addons">
                                        <button class="btn btn-outline-primary" type="button">Search</button>
                                        <button class="btn btn-outline-primary" type="button"
                                            onclick="exportExcel('supplierRequest_list_table','supplierRequest_list_file','supplierRequest_list_sheet');">Excel
                                            Download</button>
                                        <button class="btn btn-outline-primary" type="button"
                                            onclick="location.href='/supplierRequest/add';">+ Add
                                            New</button>
                                    </div>
                                </div>

                                <div class="row layout-top-spacing">


                                    <div class="table-responsive">
                                        <table class="table table-hover table-striped table-bordered"
                                            id="supplierRequest_list_table">
                                            <thead>
                                                <tr>
                                                    <!-- <th scope="col" width="5%">
                                        <div class="form-check form-check-primary">
                                            <input class="form-check-input" id="custom_mixed_parent_all" type="checkbox">
                                        </div>
                                    </th> -->
                                                    <th scope="col">Company Name</th>
                                                    <th scope="col">Company DO</th>
                                                    <th class="text-center" scope="col">Type</th>
                                                    <th class="text-center" scope="col">Car Number</th>
                                                    <th class="text-center" scope="col">Car Name</th>
                                                    <th class="text-center" scope="col">Car Cost</th>
                                                    <th class="text-center" scope="col">Client Name</th>
                                                    <th class="text-center" scope="col">Year</th>
                                                    <th class="text-center" scope="col">Comeon Date</th>
                                                    <th class="text-center" scope="col">Comeon R Date</th>
                                                    <th class="text-center" scope="col">Collect Pay</th>
                                                    <th class="text-center" scope="col">Function</th>
                                                </tr>
                                                <tr aria-hidden="true" class="mt-3 d-block table-row-hidden"></tr>
                                            </thead>
                                            <tbody>
                                                <% function formatDate(d_t) { let year=d_t.getFullYear(); let month=("0"
                                                    + (d_t.getMonth() + 1)).slice(-2); let day=("0" +
                                                    d_t.getDate()).slice(-2); let hour=("0" + d_t.getHours()).slice(-2);
                                                    let minute=("0" + d_t.getMinutes()).slice(-2); let seconds=("0" +
                                                    d_t.getSeconds()).slice(-2); return year + "-" + month + "-" + day
                                                    + " " + hour + ":" + minute + ":" + seconds } %>
                                                    <% data.forEach( function(item, index) { %>
                                                        <tr
                                                            onclick="location.href='/supplierRequest/<%= item.supplierRequest_id %>';">
                                                            <!-- <td>
                                        <div class="form-check form-check-primary">
                                            <input class="form-check-input custom_mixed_child" type="checkbox">
                                        </div>
                                    </td> -->
                                                            <td class="text-center">
                                                                <span class="media-body align-self-center">
                                                                    <%= item.companyName %>
                                                                </span>
                                                            </td>
                                                            <td class="text-center">
                                                                <span class="media-body align-self-center">
                                                                    <%= item.collectMethod || '' %>
                                                                </span>
                                                            </td>
                                                            <td class="text-center">
                                                                <span class="media-body align-self-center">
                                                                    <%= item.disposalType || '' %>
                                                                </span>
                                                            </td>
                                                            <td class="text-center">
                                                                <span class="media-body align-self-center">
                                                                    <%= item.carNum %>
                                                                </span>
                                                            </td>
                                                            <td class="text-center">
                                                                <span class="media-body align-self-center">
                                                                    <%= item.model %>
                                                                </span>
                                                            </td>
                                                            <td class="text-center">
                                                                <span class="media-body align-self-center">
                                                                    <%= item.amount1 || '' %>
                                                                </span>
                                                            </td>
                                                            <td class="text-center">
                                                                <span class="media-body align-self-center">
                                                                    <%= item.ownerName %>
                                                                </span>
                                                            </td>
                                                            <td class="text-center">
                                                                <span class="media-body align-self-center">
                                                                    <%= item.yearModel %>
                                                                </span>
                                                            </td>
                                                            <td class="text-center">
                                                                <span class="media-body align-self-center">
                                                                    <%= item.collectReqDate ?
                                                                        formatDate(item.collectReqDate) : '' %>
                                                                </span>
                                                            </td>
                                                            <td class="text-center">
                                                                <span class="media-body align-self-center">
                                                                    <%= item.collectReqDate ?
                                                                        formatDate(item.collectReqDate) : '' %>
                                                                </span>
                                                            </td>
                                                            <td class="text-center">
                                                                <span class="media-body align-self-center">
                                                                    <%= item.disposalFeeYN? 'O' : 'X' %>
                                                                </span>
                                                            </td>

                                                            <td class="text-center">
                                                                <div class="action-btns">
                                                                    <span
                                                                        onclick="pdf_print(event, '<%= item.supplierRequest_id %>')"
                                                                        class="action-btn btn-view bs-tooltip me-2"
                                                                        data-toggle="tooltip" data-placement="top"
                                                                        title="View">
                                                                        <svg xmlns="http://www.w3.org/2000/svg"
                                                                            width="24" height="24" viewBox="0 0 24 24"
                                                                            fill="none" stroke="currentColor"
                                                                            stroke-width="2" stroke-linecap="round"
                                                                            stroke-linejoin="round"
                                                                            class="feather feather-eye">
                                                                            <path
                                                                                d="M1 12s4-8 11-8 11 8 11 8-4 8-11 8-11-8-11-8z">
                                                                            </path>
                                                                            <circle cx="12" cy="12" r="3"></circle>
                                                                        </svg>
                                                                    </span>
                                                                    <a href="javascript:void(0);"
                                                                        class="action-btn btn-edit bs-tooltip me-2"
                                                                        data-toggle="tooltip" data-placement="top"
                                                                        title="Edit">
                                                                        <svg xmlns="http://www.w3.org/2000/svg"
                                                                            width="24" height="24" viewBox="0 0 24 24"
                                                                            fill="none" stroke="currentColor"
                                                                            stroke-width="2" stroke-linecap="round"
                                                                            stroke-linejoin="round"
                                                                            class="feather feather-edit-2">
                                                                            <path
                                                                                d="M17 3a2.828 2.828 0 1 1 4 4L7.5 20.5 2 22l1.5-5.5L17 3z">
                                                                            </path>
                                                                        </svg>
                                                                    </a>
                                                                    <a href="javascript:void(0);"
                                                                        class="action-btn btn-delete bs-tooltip"
                                                                        data-toggle="tooltip" data-placement="top"
                                                                        title="Delete">
                                                                        <svg xmlns="http://www.w3.org/2000/svg"
                                                                            width="24" height="24" viewBox="0 0 24 24"
                                                                            fill="none" stroke="currentColor"
                                                                            stroke-width="2" stroke-linecap="round"
                                                                            stroke-linejoin="round"
                                                                            class="feather feather-trash-2">
                                                                            <polyline points="3 6 5 6 21 6"></polyline>
                                                                            <path
                                                                                d="M19 6v14a2 2 0 0 1-2 2H7a2 2 0 0 1-2-2V6m3 0V4a2 2 0 0 1 2-2h4a2 2 0 0 1 2 2v2">
                                                                            </path>
                                                                            <line x1="10" y1="11" x2="10" y2="17">
                                                                            </line>
                                                                            <line x1="14" y1="11" x2="14" y2="17">
                                                                            </line>
                                                                        </svg>
                                                                    </a>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <% }); %>
                                            </tbody>
                                        </table>
                                    </div>

                                </div>

                            </div>

                        </div>

                        <%- include ("../partials/footer.ejs") %>

                    </div>
                    <!--  END CONTENT AREA  -->
            </div>
            <!-- END MAIN CONTAINER -->

            <%- include ("../partials/commonScrpits.ejs") %>

                <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM SCRIPTS -->
                <script>
                    function pdf_print(evnet, id) {
                        event.stopPropagation();
                        alert(`${id} pdf 출력??`);
                    }
                </script>
                <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM SCRIPTS -->
</body>

</html>