<!DOCTYPE html>
<html lang="en">

<head>
  <%- include ("../partials/header.ejs") %>
</head>

<body class="layout-boxed alt-menu">

  <%- include ("../partials/loader.ejs") %>

    <%- include ("../partials/nav.ejs") %>

      <!--  BEGIN MAIN CONTAINER  -->
      <div class="main-container sidebar-closed " id="container">

        <div class="overlay"></div>
        <div class="search-overlay"></div>

        <%- include ("../partials/sidebar.ejs") %>

          <!--  BEGIN CONTENT AREA  -->
          <div id="content" class="main-content">
            <div class="layout-px-spacing">

              <div class="middle-content container-xxl p-0">
                <!-- BREADCRUMB -->
                <div class="page-meta">
                  <nav class="breadcrumb-style-one" aria-label="breadcrumb">
                    <ol class="breadcrumb">
                      <li class="breadcrumb-item"><a href="/parts">부속품 정보 관리</a></li>
                      <li class="breadcrumb-item active" aria-current="page">
                        <%= data.name %>
                      </li>
                      <li class="breadcrumb-item active" aria-current="page">수정</li>
                    </ol>
                  </nav>
                </div>
                <!-- /BREADCRUMB -->

                <div class="row layout-spacing">
                  <div class="col-lg-12 layout-top-spacing">
                    <div class="col-lg-12">
                      <form method="post">
                        <div class="statbox widget box box-shadow">
                          <div class="widget-header">
                            <div class="row">
                              <div class="col-xl-12 col-md-12 col-sm-12 col-12">
                                <h4></h4>
                              </div>
                            </div>
                          </div>

                          <div class="widget-content widget-content-area">
                            <div class="row">
                              <div class="col-xl-12 col-md-12 col-sm-12 col-12">
                                <h6>품명</h6>
                              </div>
                            </div>

                            <div class="row layout-spacing">
                              <div class="col-lg-6 col-12 ">
                                <div class="form-group">
                                  <label for="text1" class="visually-hidden">Text</label>
                                  <input id="name" type="text" name="name" placeholder="" class="form-control" required value="<%= data.name %>">
                                </div>
                              </div>
                            </div>

                            <div class="row">
                              <div class="col-xl-12 col-md-12 col-sm-12 col-12">
                                <h6>단위</h6>
                              </div>
                            </div>

                            <div class="row layout-spacing">
                              <div class="col-lg-6 col-12 ">
                                <div class="form-group">
                                  <label for="text2" class="visually-hidden">Text</label>
                                  <input id="unit" type="text" name="unit" placeholder="" class="form-control" required value="<%= data.unit %>">
                                </div>
                              </div>
                            </div>

                            <div class="row">
                              <div class="col-xl-12 col-md-12 col-sm-12 col-12">
                                <h6>단위당 가격</h6>
                              </div>
                            </div>

                            <div class="row layout-spacing">
                              <div class="col-lg-6 col-12 ">
                                <div class="form-group">
                                  <label for="text3" class="visually-hidden">Text</label>
                                  <input id="price" type="text" name="price" placeholder="" class="form-control"
                                    required value="<%= data.price %>">
                                </div>
                              </div>
                            </div>

                            <div class="row">
                              <div class="col-xl-12 col-md-12 col-sm-12 col-12">
                                <h6>메모</h6>
                              </div>
                            </div>

                            <div class="row layout-spacing">
                              <div class="col-lg-6 col-12 ">
                                <div class="form-group">
                                  <label for="text4" class="visually-hidden">Text</label>
                                  <input id="note" type="text" name="note" placeholder="" class="form-control" required value="<%= data.note %>">
                                </div>
                              </div>
                            </div>

                            <div class="row layout-top-spacing">
                              <div class="col-lg-6 col-12 ">
                                <button class="btn btn-outline-success mb-2 me-4">저장</button>
                                <button class="btn btn-outline-danger mb-2 me-4" type="button"
                                  onclick="cancel()">취소</button>
                              </div>
                            </div>
                          </div>
                        </div>
                      </form>
                    </div>
                  </div>



                </div>
              </div>

            </div>


            <%- include ("../partials/footer.ejs") %>

          </div>
          <!--  END CONTENT AREA  -->
      </div>
      <!-- END MAIN CONTAINER -->


      <%- include ("../partials/commonScrpits.ejs") %>

        <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM SCRIPTS -->

        <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM SCRIPTS -->
</body>

</html>