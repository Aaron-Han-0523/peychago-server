                                                                    <div class="form-group">
                                                                        <input class="input-date form-control"
                                                                            type="text" name="date4"
                                                                            value="<%= formatDate(process.date4) %>">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div class="form-group row">
                                                            <div class="row">
                                                                <div class="col col-xl-4 mb-xl-0 mb-2">
