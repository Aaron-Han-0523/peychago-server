                                                            </div>
                                                            <div class="p-2 mb-xl-1 mb-4">
                                                                <select class="form-select" type="text" id="state"
                                                                    name="state">
                                                                    <% for(code in exportStateCode2Text) { %>
                                                                        <% if (process.state==code) { %>
                                                                            <option value="<%= code %>" selected>
                                                                                <%= exportStateCode2Text[code] %>
                                                                            </option>
                                                                            <% } else { %>
                                                                                <option value="<%= code %>">
                                                                                    <%= exportStateCode2Text[code] %>
                                                                                </option>
                                                                                <% } %>
                                                                                    <% } %>
